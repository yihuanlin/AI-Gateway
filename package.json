{
  "name": "vercel-ai-gateway",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --watch --env-file=.env.local dist/index.js --port 8888",
    "build": "tsc",
    "build:netlify": "mkdir -p netlify/edge-functions && cp src/index.ts netlify/edge-functions/index.ts && cp -R src/modules netlify/edge-functions/ && cp -R src/shared netlify/edge-functions/",
    "watch:netlify": "fswatch -o src | xargs -n1 -I{} netlify build",
    "dev:netlify": "trap 'jobs -p | xargs -r kill' EXIT; (bun run watch:netlify &) && netlify build && netlify dev",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^1.0.11",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/groq": "^2.0.14",
    "@ai-sdk/openai": "^2.0.19",
    "@ai-sdk/openai-compatible": "^1.0.11",
    "@huggingface/inference": "^4.7.1",
    "@netlify/blobs": "^10.0.8",
    "ai": "^5.0.22",
    "hono": "^4.9.4",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}